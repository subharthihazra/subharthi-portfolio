<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="./index.css" />
    <title>Subharthi's Portfolio</title>
    <meta name="theme-color" content="#000000" />

    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Quicksand:wght@300;400;500;600&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@300;400;500;600;700;800;900&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Mooli&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Epilogue:wght@300;400;500;600;700;800&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Comfortaa:wght@300;400;500;600&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
    />
  </head>
  <body>
    <div id="container">
      <div id="header">
        <div id="text">sh</div>
        <div id="navbar">
          <a href="#first_banner"><div class="navbar_link">Home</div></a>
          <a href="#skills"><div class="navbar_link">Skills</div></a>
          <a href="#projects"><div class="navbar_link">Projects</div></a>
          <!-- <a href="#experience"><div class="navbar_link">Experience</div></a> -->
          <!-- <a href="#education"><div class="navbar_link">Education</div></a> -->
          <a href="#contact"><div class="navbar_link">Contact</div></a>
          <!-- <a href="#blogs"><div class="navbar_link">Blogs</div></a> -->
        </div>
        <div id="menu_button">
          <div id="menu_icon">
            <span class="line" id="line1"></span>
            <span class="line" id="line2"></span>
            <span class="line" id="line3"></span>
          </div>
        </div>
      </div>

      <div id="menu">
        <div id="wrapper"></div>
      </div>

      <div id="main">
        <div id="first_banner" class="section">
          <div id="text_layer" class="layer">
            <img
              src="./images/mypic.jpg"
              width="100"
              height="100"
              id="mypic"
              alt=""
            />
            <div id="text">
              I'm <span id="name">Subharthi Hazra</span>, a
              <span id="prof">Software Developer</span>
              from Kolkata, India !
            </div>
            <div id="links">
              <div id="wrapper">
                <a href="https://twitter.com/SubharthiHazra"
                  ><span
                    class="item fa-brands fa-x-twitter"
                    style="--icon-color: #111111"
                  ></span>
                </a>
                <a href="https://www.linkedin.com/in/subharthihazra/">
                  <span
                    class="item fa-brands fa-linkedin-in"
                    style="--icon-color: #0d52c5"
                  ></span>
                </a>
                <a href="https://github.com/subharthihazra">
                  <span
                    class="item fa-brands fa-github"
                    style="--icon-color: #111111"
                  ></span>
                </a>
                <a href="mailto:subharthi75@gmail.com">
                  <span
                    class="item fa-regular fa-envelope"
                    style="--icon-color: #285d4c"
                  ></span>
                </a>
                <!-- <div class="item" id="youtube" style="--icon-color: red"></div> -->
                <!-- <div
                  class="item"
                  id="discord"
                  style="--icon-color: purple"
                ></div> -->
                <!-- <div class="item"></div> -->
              </div>
            </div>
          </div>

          <div id="pic_layer" class="layer"></div>
          <div id="scroll_down">
            <div id="wrapper">
              <div id="logo"></div>
            </div>
          </div>
          <div id="pointer_circle">
            <div id="actual_circle"></div>
          </div>
        </div>
        <div id="skills" class="section cards_section hidden">
          <div id="inner">
            <div id="skill_text" class="text">Skills</div>
            <%
            //////////////////////////////////////////////////////////////////////////////
            let tot = 0;
            //////////////////////////////////////////////////////////////////////////////
            for(let i = 0; i < portfolioData.skills.length; i++) {
            //////////////////////////////////////////////////////////////////////////////
            tot += portfolioData.skills[i].group_elms.length;
            //////////////////////////////////////////////////////////////////////////////
            } %>
            <div
              id="skills_shelf"
              class="shelf"
              style="--card-count: <%= tot %>"
            >
              <%
              //////////////////////////////////////////////////////////////////////////////
              let cnt = 0;
              //////////////////////////////////////////////////////////////////////////////
              for (let i = 0; i < portfolioData.skills.length; i++) {
              //////////////////////////////////////////////////////////////////////////////
              skill_group = portfolioData.skills[i];
              //////////////////////////////////////////////////////////////////////////////
              %>
              <div class="group">
                <div class="group_name"><%= skill_group.group_name %></div>
                <div class="group_items">
                  <% for (let j = 0; j < skill_group.group_elms.length; j++,
                  cnt++) { const skill = skill_group.group_elms[j]; %>
                  <div
                    class="item"
                    style="
                      --skill-level-bar-width: <%= skill.level %>%;
                      --card-color: <%= skill.color %>;
                      --card-iterator: <%= cnt %>;
                    "
                  >
                    <div class="top">
                      <div
                        class="image"
                        style="background-image: url('<%= skill.imgUrl %>')"
                      ></div>
                      <div class="name"><%= skill.name %></div>
                    </div>
                    <% if(skill.level){ %>
                    <div class="level">
                      <div class="bar"></div>
                    </div>
                    <% } %>
                  </div>
                  <% } %>
                </div>
              </div>
              <% } %>
            </div>
          </div>
        </div>

        <div id="projects" class="section cards_section hidden">
          <div id="inner">
            <script type="text/javascript">
              let projectDatas = {};
            </script>
            <div id="project_text" class="text">Projects</div>
            <div
              id="project_shelf"
              class="shelf"
              style="--card-count: <%= portfolioData.projects.length %>"
            >
              <% for (let i = 0; i < portfolioData.projects.length; i++) { //fg
              const project = portfolioData.projects[i]; %>
              <div
                class="item"
                style="
                  --card-color: <%= project.color %>;
                  --card-iterator: <%= i %>;
                "
              >
                <div
                  class="image"
                  style="background-image: url('<%= project.imgUrl %>')"
                ></div>
                <div class="name"><%= project.name %></div>
                <div id="links">
                  <% if(project.githublink){ %>
                  <a href="<%= project.githublink %>" target="_blank">
                    <div id="project_githublink" class="link"></div>
                  </a>
                  <% } %> <% if(project.videolink){ %>
                  <a href="<%= project.videolink %>" target="_blank">
                    <div id="project_videolink" class="link"></div>
                  </a>
                  <% } %> <% if(project.livelink){ %>
                  <a href="<%= project.livelink %>" target="_blank">
                    <div id="project_livelink" class="link"></div>
                  </a>
                  <% } %>
                </div>
                <script type="text/javascript">
                  projectDatas[<%= i %>] = {};
                  projectDatas[<%= i %>].description = `<%= project.description %>`;
                  projectDatas[<%= i %>].techstacks = [<%
                   for(techstack of project.techstacks){
                    %>"<%= techstack %>",<%
                   }
                  %>];
                </script>
              </div>
              <% } %>
            </div>
          </div>
        </div>

        <div id="contact" class="section">
          <div id="inner">
            <div id="contact_text" class="text">Contact Me</div>
            <p id="desc">
              If you have any inquiries, collaboration proposals, or if you
              simply want to connect, feel free to reach out. I'm always open to
              new opportunities (freelance, internship, job). My inbox is always
              open.
            </p>

            <div id="links">
              <div id="wrapper">
                <a href="https://twitter.com/SubharthiHazra"
                  ><span
                    class="item fa-brands fa-x-twitter"
                    style="--icon-color: #111111"
                  ></span>
                </a>
                <a href="https://www.linkedin.com/in/subharthihazra/">
                  <span
                    class="item fa-brands fa-linkedin-in"
                    style="--icon-color: #0d52c5"
                  ></span>
                </a>
                <a href="https://github.com/subharthihazra">
                  <span
                    class="item fa-brands fa-github"
                    style="--icon-color: #111111"
                  ></span>
                </a>
                <a href="mailto:subharthi75@gmail.com">
                  <span
                    class="item fa-regular fa-envelope"
                    style="--icon-color: #285d4c"
                  ></span>
                </a>
                <!-- <div class="item" id="youtube" style="--icon-color: red"></div> -->
                <!-- <div
                  class="item"
                  id="discord"
                  style="--icon-color: purple"
                ></div> -->
                <!-- <div class="item"></div> -->
              </div>
            </div>

            <h3>Let's Connect</h3>

            <!-- Contact Form -->
            <form method="post">
              <input
                type="text"
                name="name"
                placeholder="Your name (optional)"
              />
              <textarea
                id="message"
                name="message"
                rows="4"
                placeholder="Send me a direct message ..."
                required
              ></textarea>
              <button class="submit">
                <span class="fa-regular fa-paper-plane"></span>
              </button>
            </form>
          </div>
        </div>
      </div>

      <div id="footer">
        Made with <span class="fa-solid fa-heart"></span> by Subharthi Hazra
      </div>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/1.1.9/p5.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vanta@latest/dist/vanta.topology.min.js"></script>
    <script src="https://unpkg.com/kinet@2.2.1/dist/kinet.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.6.5/axios.min.js"></script>
    <script type="text/javascript" src="./index.js"></script>
  </body>
</html>
